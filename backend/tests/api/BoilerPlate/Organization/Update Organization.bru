meta {
  name: Update Organization
  type: http
  seq: 3
}

put {
  url: {{baseUrl}}/organization/update
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
          name: organizationName.value,
        email: organizationEmail.value,
        logo: organizationLogo.value,
  
  }
}
