meta {
  name: Modifier une classe
  type: http
  seq: 3
}

put {
  url: {{baseUrl}}/classes/1
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "name": "Nouvelle classe modifiée"
  }
}

tests {
  test("devrait modifier une classe", function() {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
    expect(data.message).to.equal('Classe modifiée avec succès');
  });
}
